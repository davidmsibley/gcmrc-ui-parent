<configuration>
	<appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%t] NUDE %5p %logger{20} - %m%n</pattern>
		</encoder>
	</appender>
	<logger name="gov.usgs.webservices.jdbc.spec" level="TRACE"/>
	<root level="DEBUG">
		<appender-ref ref="stdout" />
	</root>
</configuration>